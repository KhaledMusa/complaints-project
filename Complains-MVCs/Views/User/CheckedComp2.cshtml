@model Complains_MVCs.Models.FileComp

@{
    ViewData["Title"] = "CheckedComp";
    Layout = "~/Views/Shared/_Layout.cshtml";
  
}
<h1>Edit</h1>

<h4>WebUser</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CheckedComp" asp-controller="User" method="post">


            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control"  disabled />

            </div>
            <div class="form-group">
                <label asp-for="Text" class="control-label"></label>
                <input asp-for="Text" class="form-control"   />

            </div>
            <div class="form-group">
                <label asp-for="ContentType" class="control-label"></label>
                <input asp-for="ContentType" class="form-control"   />

            </div>
            <div class="form-group">
                <label asp-for="fileName" class="control-label"></label>
                <input asp-for="fileName" class="form-control"   />

            </div>
            <div class="form-group">
                <label asp-for="fileUp" class="control-label">Image</label>


                <a href="~/Uploads/@Model.fileName" download>
                        <span class="btn btn-primary">
                            DownLoad
                        </span>
                    
                    </a>
               
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" value="Accepted" />

            </div>
            <div class="form-group">
                <label asp-for="UserId" class="control-label"></label>
                <input asp-for="UserId" class="form-control"   />

            </div>

            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
