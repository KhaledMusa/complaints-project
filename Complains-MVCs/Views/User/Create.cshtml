            </div>
            <div class="form-group">
                <label asp-for="UserId" class="control-label"></label>
                <input asp-for="UserId" class="form-control" value="@ViewBag.UserObjectJson" disabled />
                <span asp-validation-for="UserId" class="text-danger"></span>
